        $(document).ready(function() {
            function isTouchDevice(){
                return true == ("ontouchstart" in window || window.DocumentTouch && document instanceof DocumentTouch);
            }

            if(isTouchDevice()===false) {
                tooltip();

                $.getJSON("/home-json-tooltips-html", function (manga) {
                    var id = "";
                    var str_id_isset = ",";
                    $('a.tooltip').hover(function (e) {
                        if (!$(this).hasClass('hastool')) {
                            id = $(this).attr('data-tooltip');
                            $.each(manga, function (key, val) {
                                if ("sticky_" + val.id == id && str_id_isset.indexOf("," + val.id + ",") == -1) {
                                    str_id_isset += val.id + ",";
                                    $tooltip_add = '<div id="' + id + '" class="atip"> <div id="tooltip"> <h3>' + val.name + '</h3> <div class="message_main"> <div class="message_mainT"> <div class="message_mainI"> <img title="' + val.name + '" src="' + val.image + '" onerror="javascript:this.src=\'//truyenmoi.xyz/themes/home/images/404-avatar.png\';"> </div> <div class="message_main_text"> <p> <label>Tác giả:</label>' + val.author + '</p> <p> <label>Cập nhật mới:</label>' + val.updatetime + '</p> <p> <label>Lượt đọc :</label>' + val.view + '</p> <p> <label>Thể loại :</label>' + val.genres + '</p> </div> </div> <div class="message_mainB"> <span>Nội dung: </span>' + val.description + '</div> </div> </div> </div>';
                                    $('#mystickytooltip').append($tooltip_add);
                                    return false;
                                }
                            });
                            $(this).addClass('hastool');
                        }
                        $(this).attr("title", "");
                    }, function () {
                    });
                });
            }
        });
    </script>
