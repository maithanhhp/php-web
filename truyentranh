<script>
    function openCity(cityName) {
        var i;
        var x = document.getElementsByClassName("city");

        for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }

        document.getElementById(cityName).style.display = "block";

        $('.w3-bar-item').removeClass('active');

        if (cityName == 'TopMonth') {
            $('#TopMonthBtn').addClass('active');
        } else if (cityName == 'TopWeek') {
            $('#TopWeekBtn').addClass('active');
        } else if (cityName == 'TopDay') {
            $('#TopDayBtn').addClass('active');
        }

        if ($('#'+cityName).find('.loading').length) {
            $.ajax({
                type: 'post',
                data: {
                    cityName : cityName
                },
                dataType: 'html',
                url: 'https://truyenmoi.xyz/get-top-manga',
                success: function (response) {
                    if (cityName == 'TopMonth') {
                        $('#topMonth').html(response);
                    } else if (cityName == 'TopWeek') {
                        $('#topWeek').html(response);
                    } else if (cityName == 'TopDay') {
                        $('#topDay').html(response);
                    }
                }
            });
        }
    }
</script>
