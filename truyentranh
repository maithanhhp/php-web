<script>
    $(document).ready(function () {
        $('.close-ad-balloon-left').click(function () {
            $(this).parent().hide();
            var date = new Date();
            var minutes = 15;
            date.setTime(date.getTime() + (minutes * 60 * 1000));
            $.cookie("show-balloon-left", "yes", { expires: date, path: '/' });
        });

        var showBalloonLeft = $.cookie("show-balloon-left");
        if (!showBalloonLeft) {
            $('.ad-pc-balloon-left').show();
        }
    })
</script>
