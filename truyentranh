        if (currentUrl.pathname == '/') {
            if (currentUrl.hash == '#daily-update') {
                $('#siteBottomNav .each').removeClass('active');
                $('#siteBottomNav .newest').addClass('active');
            } else if (currentUrl.hash == '#search') {
                $('#search_story2').focus();
