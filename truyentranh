                                last3chaps = mangas[i]['last3chaps'];

                                html += '<div class="itemupdate first">' +
                                    '<a rel="nofollow" data-tooltip="sticky_" class="tooltip cover a-home" href="'+'/doc-truyen-tranh-'+manga['slug']+'">' +
                                    '<img src="'+manga['thumb']+'" onerror="'+errorJs+'" width="60" height="85" alt="'+manga['name']+'">' +
                                    '</a>' +
                                    '<ul>' +
                                    '<li>' +
                                    '<h3><a class="tooltip hastool" data-tooltip="sticky_" href="'+'/doc-truyen-tranh-'+manga['slug']+'" title="">'+manga['name']+'</a></h3>' +
                                    '<a class=" textdecoration"></a>' +
                                    '<a class="non"></a>' +
                                    '</li>';

                                for (var j=0;j<last3chaps.length;j++) {
                                    html += '<li>' +
                                        '<span><a class="sts sts_1" href="'+'/'+manga['slug']+'/doc-truyen-tranh-'+last3chaps[j]['slug']+'" title="'+last3chaps[j]['name']+'">'+last3chaps[j]['chap_text']+'</a></span>' +
                                        '<i>'+last3chaps[j]['date_release']+'</i>' +
                                        '</li>';
                                }

                                html += '</ul>' +
                                    '</div>';
                            }

                            $('.doreamon').append(html);
                        }

                        $('.loadingGif').hide();
                    }
                });
            });
        })
    </script>
</body>
</html>
